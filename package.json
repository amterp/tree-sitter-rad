{
  "name": "tree-sitter-rad",
  "version": "0.1.1",
  "description": "A parser for Rad, a modern CLI scripting language.",
  "repository": "https://github.com/amterp/tree-sitter-rad",
  "license": "MIT",
  "author": {
    "name": "Alexander Terp",
    "email": "alexander.terp@gmail.com",
    "url": "https://github.com/amterp"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "rad"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.23"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "buildd": "tree-sitter generate && npm install",
    "testt": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.rad",
      "injection-regex": "^rad$"
    }
  ]
}
